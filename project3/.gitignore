# Node.js and Express.js template .gitignore

# Ignore node_modules everywhere
**/node_modules/

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Environment variables
.env
.env.test
.env.local

# OS generated files
.DS_Store
Thumbs.db

# Editor folders
.vscode/
.idea/

# Coverage directory
coverage/

# Build output
dist/
build/

# TypeScript cache
*.tsbuildinfo

# Optional npm cache
.npm/

# Debugging files
*.local

# ESLint cache
.eslintcache
